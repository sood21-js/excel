 {
     "parser": "babel-eslint",
     "env": {
         "es6": true,
         "browser": true,
         "node": true
     },
     "rules": {
        "semi":"off",
        "arrow-parens":"off",
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "comma-dangle":"off",
        "operator-linebreak": "off",
        "no-debugger": "warn",
        "indent": ["error", 4, { "SwitchCase": 1 }]
     },
     "extends": ["eslint:recommended", "google"]
 }